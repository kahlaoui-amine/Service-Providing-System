{% extends 'base.html.twig' %}

{% block title %}Hello IndexController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
   <p>VilleId:{{ ville.getVilleId }}</p>
   <p>VilleNom:{{ ville.getVilleNom }}</p>
   <p>VilleDate:{{ ville.getVilleDate }}</p>
   <p>VilleDepartement:{{ ville.getVilleDepartement }}</p>
   <p>VilleSlug:{{ ville.getVilleSlug }}</p>
   <p>VilleNomSimple:{{ ville.getVilleNomSimple }}</p>
   <p>VilleNomReel:{{ ville.getVilleNomReel }}</p>
   <p>VilleNomSoundex:{{ ville.getVilleNomSoundex }}</p>
   <p>VilleNomMetaphone:{{ ville.getVilleNomMetaphone }}</p>
   <p>VilleCodePostal:{{ ville.getVilleCodePostal }}</p>
   <p>VilleCommune:{{ ville.getVilleCommune }}</p>
   <p>VilleCodeCommune:{{ ville.getVilleCodeCommune }}</p>
   <p>VilleArrondissement:{{ ville.getVilleArrondissement }}</p>
   <p>VilleCanton:{{ ville.getVilleCanton }}</p>
   <p>VilleAmdi:{{ ville.getVilleAmdi }}</p>
</div>
{% endblock %}
